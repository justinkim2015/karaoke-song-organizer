
<div class="box">
  <h1 class='text-xl mb-2'><%= @place.name %></h1>

  <iframe
    width="100%"
    height="400px"
    style="border:0"
    loading="lazy"
    allowfullscreen
    src= <%= @map_url %>>
  </iframe>
</div>

<div class="box">
  <h1 class='text-xl mb-2'>Hours</h1>
  <%= @status %>
  <ul>
    <% @hours.each do |day| %>
      <li><%= day %></li>
    <% end %>
  </ul>
</div>

